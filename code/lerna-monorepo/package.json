{
  "name": "npm-lerna-demo",
  "main": "",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "npx lerna run bootstrap",
    "build": "npx lerna run build",
    "install:web": "npx lerna exec --scope people-admin -- pnpm install",
    "dev:web": "npx lerna exec --scope people-admin -- yarn dev",
    "test:validat-utils": "lerna exec --scope javascript-validate-utils -- yarn test",
    "publish:repo": "lerna publish --skip-git --conventional-commits"
  },
  "devDependencies": {
    "lerna": "^8.1.2"
  },
  "version": "0.0.3"
}
